{
	"compilerOptions": {
		"module": "ESNext",
		"target": "ES2022",
		"moduleResolution": "node",
		"noEmit": true,
		"strict": true,
		//"noUnusedLocals": true,
		//"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true
	},
	"include": ["cms"]
}